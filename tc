//gold2 1208
#include <cstdio>
#include <vector>
using namespace std;
int N,S;
const int mxN = 4e5+1;
vector<int> a[2];
long long dp[2][mxN];

void print(){
    puts("plus");
    for(int i=0;i<mxN;i++){
        if(dp[0][i]) printf("i:%d, cnt:%d\n",i,dp[0][i]);
    }

    puts("\n\nminus");
    for(int i=0;i<mxN;i++){
        if(dp[1][i]) printf("i:%d, cnt:%d\n",i,dp[1][i]);
    }
    puts("");
}


int main(){
    scanf("%d%d",&N,&S);
    for(int i=0;i<N;i++) {
        int v;
        scanf("%d",&v);
        if(v>0) a[0].push_back(v);
        else a[1].push_back(-v);
        
    }

    int k=2;
    while(k--){
        for(int i=0;i<a[k].size();i++){
            for(int j=mxN;j>=0;j--){
                dp[k][j]+=(j-a[k][i]==0);
                if(j-a[k][i]>0) dp[k][j] += dp[k][j-a[k][i]];
            }
        }
    }

    print();
    long long ans = 0;
    // if(S>=0){
    //     for(int i=0;i<=mxN;i++){
    //         if(!dp[0][i]) continue;
    //         if(S==i) ans+=dp[0][i];
    //         else if(S<i && i-S <= mxN && dp[1][i-S]) ans+=dp[0][i]*dp[1][i-S]; 
    //     }
    // }else{
    //     for(int i=0;i<=mxN;i++){
    //         if(!dp[1][i]) continue;
    //         if(S==-i) ans+=dp[1][i];
    //         else if(S>i && S-i <= mxN && dp[0][S-i]) ans+=dp[1][i]*dp[0][S-i]; 
    //     }        
    // }
    printf("%lld",ans);
}
